package poker;

/**

 * Esta clase lanza a consola el método robar carta

 * @author: Pilar

 * @version: 22/09/2016/A

 * @see <a href = "http://www.aprenderaprogramar.com" /> aprenderaprogramar.com – Didáctica en programación </a>

 */
public class Poker { 
  public static void main(String[] args) {
    try {
      Baraja b = new Baraja();
      for (int i = 0; i < 52; i++) {
        System.out.println(b.robarCarta());
      }
      System.out.println();
      Baraja b1 = new Baraja();
      b1.mezclarBaraja();
      for (int i = 0; i < 52;i++) {
        System.out.println(b1.robarCarta());
      }
      System.out.println();
    } catch (Exception ex) {
      System.out.println(ex);
    }
  }
}
